.container {
  padding: 0 40rpx;
}
.album-name {
  margin-bottom: 20rpx;
 font-size: 36rpx;
}
.album-photos {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.album-photos::after{
  content: '';
  width: 200rpx;
}
.album-photos .photo {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}
.album-btn {
  position: fixed;
  bottom: 0;
  left:0;
  right:0;
  padding: 30rpx;
}
.album-btn .btn-large{
  width: 200rpx;
}